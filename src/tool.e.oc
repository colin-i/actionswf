functionx xlog_pad_get()
	return globalr.pad
endfunction
functionx xlog_pad_set(ss ?new_pad)
	if new_pad!=0
		set globalr.pad new_pad
	else #it is div mult and at 1 is equal at 0 is bad
		set globalr.pad 1
	end
endfunction

