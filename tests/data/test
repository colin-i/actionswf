
if [ -z "${CC}" ]; then
	if [ "${conv_64}" = 1 ]; then
		e=32
	fi
else
	e=32
fi

etest=../../example/example.swf.data

if [ ! -e ${etest} ]; then
	/bin/bash ./a || exit 1
fi

set -o pipefail && /bin/bash ./a | tee data.txt || exit 1
diff ../.expected/data${e}.txt data.txt
if [ $? != 0 ]; then
if [ -n "${1}" ]; then exit 1; fi
	echo y \?; read -n1 -t $((60*60*24)) ans
	if [ "${ans}" != "y" ]; then exit 1; fi
	cat data.txt>../.expected/data${e}.txt #a mv was here but now data32.txt is a hard link and will break that
fi

diff ../.expected/example.swf.data ${etest}
if [ $? -ne 0 ]; then
	if [ -n "${1}" ]; then exit 1; fi
	source ../xxd
	xxd${xxd_args} ../.expected/example.swf.data > 1
	xxd${xxd_args} ${etest} > 2
	diff --color 1 2
	rm 1 2
	echo y \?; read -n1 -t $((60*60*24)) ans
	if [ "${ans}" != "y" ]; then exit 1; fi
	cp ${etest} ../.expected
fi

rm data.txt
