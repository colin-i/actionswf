
cd src

ver=${1}
shift

./makewin${ver} $@ && \
\
cd .. && \
mkdir actionswf && \
mv src/actionswf.dll actionswf && \
mv ${depend_dll_path}/libzlib1.dll actionswf && \
mv ${depend_dll_path}/libssp-0.dll actionswf && \
cd src && \
./makewin${ver} clean && \
cd .. && \
zip -r actionswf.zip actionswf && \
rm -r ./actionswf && \
\
unzip -l actionswf.zip
